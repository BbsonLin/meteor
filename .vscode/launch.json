    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Starlette",
            "type": "python",
            "request": "launch",
            // "program": "manage.py",
            "env": {
                "PYTHONASYNCIODEBUG": "1"
            },
            "module": "uvicorn",
            "args": [
                "startup:app", 
                "--workers", "1",
                "--loop", "uvloop",
                 "--http", "httptools",
                 "--no-access-log",
                //  If need VSCode Debugging work, need mark --reload.
                //  "--reload", 
                 
            ],
            "console": "integratedTerminal",
        }
    ]
}